{
    "id": "corto/script/ast",
    "type": "package",
    "value": {
        "description": "Create cortoscript AST from script-parser tree",
        "author": "Sander Mertens",
        "version": "1.0.0",
        "language": "c",
        "c4cpp": true,
        "use_generated_api": true,
        "keep_binary": true,
        "${os linux}": {
            "ldflags": ["-static-libstdc++", "-static-libgcc"]
        },
        "use_private": [
            "corto/script/parser",
            "antlr4/cpp"
        ]
    }
}
